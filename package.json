{
  "name": "bizbot-kenya",
  "version": "1.0.0",
  "description": "AI-powered MPESA business analytics platform for Kenyan retailers",
  "main": "server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "webpack --mode production",
    "migrate": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "db:create": "createdb bizbot_kenya_dev",
    "db:drop": "dropdb bizbot_kenya_dev",
    "db:reset": "npm run db:drop && npm run db:create && npm run migrate && npm run seed",
    "setup": "node scripts/setup.js"
  },
  "keywords": [
    "mpesa",
    "business",
    "analytics",
    "kenya",
    "ai",
    "retail",
    "voice-assistant",
    "community"
  ],
  "author": "BizBot Kenya Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "knex": "^3.0.1",
    "dotenv": "^16.3.1",
    "joi": "^17.11.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "twilio": "^4.19.0",
    "socket.io": "^4.7.4",
    "redis": "^4.6.10",
    "axios": "^1.6.2",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "csv-writer": "^1.6.0",
    "pdfkit": "^0.14.0",
    "sharp": "^0.33.0",
    "multer-s3": "^3.0.1",
    "aws-sdk": "^2.1499.0",
    "@google-cloud/speech": "^6.0.0",
    "@google-cloud/text-to-speech": "^5.0.0",
    "natural": "^6.10.4",
    "compromise": "^14.10.0",
    "node-cron": "^3.0.3",
    "winston": "^3.11.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "compression": "^1.7.4",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "babel-loader": "^9.1.3",
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-react": "^7.23.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
